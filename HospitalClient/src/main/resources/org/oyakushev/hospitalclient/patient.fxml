                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </VBox>
        </Tab>
        <Tab text="Pressure">
            <VBox>
                <HBox prefWidth="200.0">
                    <children>

                        <Button mnemonicParsing="false" prefWidth="200.0" text="New Pressure Record">
                            <HBox.margin>
                                <Insets left="10.0" top="10.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <TableView>
                    <columns>
                        <TableColumn prefWidth="75.0" text="ID">
                            <cellValueFactory>
                                <PropertyValueFactory property="id" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="150.0" text="Systolic">
                            <cellValueFactory>
                                <PropertyValueFactory property="systolic" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="150.0" text="Diastolic">
                            <cellValueFactory>
                                <PropertyValueFactory property="diastolic" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="100.0" text="Created At">
                            <cellValueFactory>
                                <PropertyValueFactory property="createdAt" />
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
