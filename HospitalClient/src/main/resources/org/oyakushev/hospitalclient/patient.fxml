<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.oyakushev.hospitalclient.controller.PatientController">
    <TabPane tabClosingPolicy="UNAVAILABLE">
        <Tab text="General">
            <VBox>
                <Label fx:id="idLabel" text="#ID" textAlignment="CENTER">
                    <font>
                        <Font size="16.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="40.0" top="10.0" />
                    </VBox.margin>
                </Label>
                <Separator>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Separator>
                <HBox>
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="First Name">
                            <HBox.margin>
                                <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="firstNameField" prefWidth="200">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>

                <HBox>
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="Last Name">
                            <HBox.margin>
                                <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="lastNameField" prefWidth="200">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>

                <HBox>
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="Birthday">
                            <HBox.margin>
                                <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </Label>
                        <DatePicker fx:id="birthdayPicker">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </DatePicker>
                    </children>
                </HBox>

                <HBox>
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="Address">
                            <HBox.margin>
                                <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="addressField" prefWidth="400">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>

                <HBox>
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="Height">
                            <HBox.margin>
                                <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="heightField">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>

                <HBox>
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="Weight">
                            <HBox.margin>
                                <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="weightField">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </TextField>
                    </children>
                </HBox>

                <Separator>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Separator>

                <HBox>
                    <children>
                        <Button fx:id="saveButton" alignment="CENTER" contentDisplay="CENTER" onAction="#onSaveAction" prefWidth="150" text="Save">
                            <HBox.margin>
                                <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="cancelButton" alignment="CENTER" contentDisplay="CENTER" onAction="#onCancelAction" prefWidth="150" text="Cancel">
                            <HBox.margin>
                                <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </VBox>
        </Tab>
        <Tab text="Blood">
            <VBox>
                <HBox prefWidth="200.0">
                    <children>

                        <Button mnemonicParsing="false" prefWidth="200.0" text="New Blood Record">
                            <HBox.margin>
                                <Insets left="10.0" top="10.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <TableView>
                    <columns>
                        <TableColumn prefWidth="75.0" text="ID">
                            <cellValueFactory>
                                <PropertyValueFactory property="id" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="100.0" text="Hemoglobin">
                            <cellValueFactory>
                                <PropertyValueFactory property="hemoglobin" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="100.0" text="Hematocrit">
                            <cellValueFactory>
                                <PropertyValueFactory property="firstName" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="100.0" text="MCV">
                            <cellValueFactory>
                                <PropertyValueFactory property="meanCorpuscularVolume" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="100.0" text="MCH">
                            <cellValueFactory>
                                <PropertyValueFactory property="meanCorpuscularHemoglobin" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="100.0" text="Created At">
                            <cellValueFactory>
                                <PropertyValueFactory property="createdAt" />
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </TableView>
                <HBox>
                    <children>
                        <Button mnemonicParsing="false" prefWidth="50.0" text="|&lt;">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" prefWidth="50.0" text="&lt;">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" />
                            </HBox.margin>
                        </Button>

                        <Label text="1 of 1 page">
                            <HBox.margin>
                                <Insets left="10.0" top="4.0" />
                            </HBox.margin>
                        </Label>

                        <Button mnemonicParsing="false" prefWidth="50.0" text="&gt;">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" prefWidth="50.0" text="&gt;|">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </VBox>
        </Tab>
        <Tab text="Examination">
            <VBox>
                <HBox prefWidth="200.0">
                    <children>

                        <Button mnemonicParsing="false" prefWidth="200.0" text="New Examination Record">
