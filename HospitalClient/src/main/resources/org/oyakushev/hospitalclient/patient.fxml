                        </TableColumn>
                        <TableColumn prefWidth="300.0" text="description">
                            <cellValueFactory>
                                <PropertyValueFactory property="description" />
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="100.0" text="Effective Time">
